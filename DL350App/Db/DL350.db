
record(stringin, "$(dev):word") #送信される波形データをword形式に変更
{
	field(DTYP, "stream")
	field(INP, "@DL350.proto word $(dev)")
	field(PINI, "YES") #IOC起動時に自動で処理
}

record(stringin, "$(dev):trigermode") #トリガーモードを即時スタートに変更
{
	field(DTYP, "stream")
	field(INP, "@DL350.proto trigermode $(dev)")
	field(PINI, "YES") #IOC起動時に自動で処理
}

record(stringin, "$(dev):end")#送信データ数の変更
{	
	field(DTYP, "stream")
	field(VAL, 100000000) 
	field(INP, "@DL350.proto end(VAL) $(dev)")
	field(SCAN, "Passive")
	field(PINI, "YES")
}

record(waveform, "$(dev):send11") #波形データを送信 添え字はチャンネルを示す
{
	field(DTYP, "stream")
	field(INP, "@DL350.proto waveformsend(1,1) $(dev)")
	field(FTVL, "SHORT")    		# データタイプ（16bit符号付き整数）
    field(NELM, "1000000")   		# 配列の長さ
	field(PROC, "0") 				# PROC対応（PROC 1でレコード処理）
	field(SCAN, "Passive") 			
	field(FLNK, "$(dev):send12")	#次のチャンネルの波形データを送信
	field(PINI, "YES")				#デバック用
}

	record(waveform, "$(dev):send12")
	{
		field(DTYP, "stream")
		field(INP, "@DL350.proto waveformsend(1,2) $(dev)")
		field(FTVL, "SHORT")   
    	field(NELM, "1000000")   
		field(SCAN, "Passive") 
		field(FLNK, "$(dev):send21")
	}

	record(waveform, "$(dev):send21")
	{
		field(DTYP, "stream")
		field(INP, "@DL350.proto waveformsend(2,1) $(dev)")
		field(FTVL, "SHORT")   
    	field(NELM, "1000000")
		field(SCAN, "Passive") 
		field(FLNK, "$(dev):send22")
	}

	record(waveform, "$(dev):send22")
	{
		field(DTYP, "stream")
		field(INP, "@DL350.proto waveformsend(2,2) $(dev)")
		field(FTVL, "SHORT")   
    	field(NELM, "1000000")
		field(SCAN, "Passive")
		field(FLNK, "$(dev):subarray11")
	}

