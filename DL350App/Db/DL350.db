record(ai, "$(dev):star")#波形取り込みスタート
{
	field(DTYP, "stream")
	field(INP, "@DL350.proto start $(dev)")
	field(SCAN,"Passive")
	field(PROC, "0")
}

record(stringin, "$(dev):stop")#波形取り込みストップ
{
	field(DTYP, "stream")
	field(INP, "@DL350.proto stop $(dev)")
	field(SCAN,"Passive")
	field(PROC, "0")
	field(FLNK, "$(dev):send11") #stop後に波形データを受け取る
}

